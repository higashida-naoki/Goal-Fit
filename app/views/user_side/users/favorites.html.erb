<h1>いいね一覧</h1>
<table class="table">
  <tbody>
    <% @favorite_posts.each do |post| %>
      <tr>
        <td><%= link_to post.created_at.strftime("%Y/%m/%d %H:%M"), post_path(post) %></td>
        <td><%= post.user.nickname %></td>
        <td><%= post.weight %></td>
        <td><%= post.user.ideal_weight %></td>
        <td><%= post.progress %></td>
      </tr>
    <% end %>
  </tbody>
</table>
